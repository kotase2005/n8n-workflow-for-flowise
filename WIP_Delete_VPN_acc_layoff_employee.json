        "jsCode": "// Lấy dữ liệu từ item đầu tiên (trong trường hợp trigger trả về mảng nhiều email)\nconst item = $json;\n\n// Ưu tiên hiển thị nội dung theo thứ tự: textAsHtml > html > text\nlet bodyHtml = '';\nlet bodyText = '';\n\nif (item.textAsHtml) {\n  bodyHtml = item.textAsHtml;\n} else if (item.html) {\n  bodyHtml = item.html;\n}\n\nif (item.text) {\n  bodyText = item.text;\n}\n\n// Trích xuất thêm thông tin tiêu đề, người gửi, người nhận nếu cần\nconst subject = item.subject || '';\nconst from = item.from?.text || item.from?.value?.[0]?.address || '';\nconst to = item.to?.text || item.to?.value?.[0]?.address || '';\nconst date = item.date || '';\n\n// Trả ra kết quả\nreturn [\n  {\n    json: {\n      subject,\n      from,\n      to,\n      date,\n      body_text: bodyText,\n      body_html: bodyHtml\n    }\n  }\n];"
      },
      "type": "n8n-nodes-base.code",
